# Settings for SX C++

CCC      = /SX/opt/sxc++/inst/bin/sxc++
CCFLAGS += -DNMWR_INCLUDE_TEMPLATE_DEFS 
CCFLAGS += -DGRAL_HAS_SSTREAM
CCFLAGS += -DSXCPP

CCFLAGS += -wall 
CCFLAGS += -woff=186  # 
CCFLAGS += -woff=997  # hiding function
CCFLAGS += -pvctl nomsg  # vectorization messages
CCFLAGS += -Tlocal -Kexceptions,rtti

LDFLAGS += -lC++_eh  #-prelink

AR=/SX/usr/bin/sxar
DEPEND    = $(CCC)
DEPFLAGS += -M $(CCFLAGS)


